{
  "name": "vscode-react-native-actions",
  "displayName": "React Native Actions",
  "description": "",
  "version": "0.0.1",
  "publisher": "lucasbsilva",
  "engines": {
    "vscode": "^1.16.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*",
    "onCommand:extension.reloadReactNative"
  ],
  "main": "./dist/index.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.reloadReactNative",
        "title": "Reload",
        "category": "React Native"
      }
    ],
    "keybindings": [
      {
        "command": "extension.reloadReactNative",
        "key": "ctrl+shift+r",
        "mac": "cmd+shift+r"
      }
    ]
  },
  "scripts": {
    "postinstall": "exit 0",
    "test": "node ./node_modules/vscode/bin/test",
    "build": "npm run clear && babel src --ignore *.spec.js,__tests__,__mocks__ --out-dir dist --copy-files",
    "clear": "rm -rf ./dist",
    "lint": "eslint ./src",
    "prepublish": "npm run build",
    "ci": "npm run lint && NODE_ENV=CI jest --runInBand",
    "watch": "babel --ignore *.spec.js,__tests__,__mocks__ -w -d ./dist ./src --copy-files"
  },
  "dependencies": {
    "socket.io": "^2.0.3"
  },
  "devDependencies": {
    "vscode": "^1.1.5",
    "babel-cli": "^6.24.0",
    "babel-core": "^6.24.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-stage-0": "^6.22.0"
  }
}